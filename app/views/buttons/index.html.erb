<!-- Image Map Generated by http://www.image-map.net/ -->
<div class="row">
  <%= image_tag("Floor Plan-Model.png", usemap: "#image-map", class: "house-img") %>
</div>

<map name="image-map">
  <% @buttons.each do |button| %>
    <area alt="<%= button.name %>" title="<%= button.name %>" href="<%= button_path(button) %>" coords="<%= button.coordinates %>" shape="rect">
  <% end %>
</map>